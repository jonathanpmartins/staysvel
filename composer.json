{
    "name": "jonathanpmartins/staysvel",
    "description": "Laravel Package for integration with Stays External API.",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Jonathan Martins",
            "email": "web@jonathanmartins.com.br"
        }
    ],
    "keywords": ["stays", "api", "integration", "laravel", "php", "pms"],
    "require": {
        "php": "^8.0.0",
        "ext-json": "*",
        "ext-curl": "*",
        "illuminate/support": "^9.46",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^9.46"

    },
    "autoload": {
        "psr-4": {
